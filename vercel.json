{
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "routes": [
    { "src": "^/$", "dest": "/simple-demo.html" },
    { "src": "^/demo$", "dest": "/simple-demo.html" }
  ],
  "rewrites": [
    {
      "source": "/whip/:path*",
      "destination": "https://demo-whip.substream.dev/:path*"
    },
    {
      "source": "/api/:path*", 
      "destination": "https://demo-api.substream.dev/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        }
      ]
    }
  ]
}
